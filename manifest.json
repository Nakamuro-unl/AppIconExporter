{
  "manifestVersion": 5,
  "id": "com.nakamuro.appiconexporter",
  "name": "App Icon Exporter",
  "version": "1.0.0",
  "main": "index.html",
  "host": [
    {
      "app": "PS",
      "minVersion": "26.0.0"
    }
  ],
  "requiredPermissions": {
    "localFileSystem": "request"
  },
  "entrypoints": [
    {
      "type": "panel",
      "id": "appIconExporterPanel",
      "label": "App Icon Exporter",
      "minimumSize": {
        "width": 300,
        "height": 400
      },
      "maximumSize": {
        "width": 800,
        "height": 1200
      },
      "preferredDockedSize": {
        "width": 350,
        "height": 500
      },
      "preferredFloatingSize": {
        "width": 400,
        "height": 600
      }
    }
  ]
}